      <animation>
    <type>select</type>
    <object-name>prop_left</object-name>
    <condition>
      <less-than>
        <property>velocities/true-airspeed-kt</property>
        <value>600</value>
      </less-than>
    </condition>
  </animation>   
 
      <animation>
    <type>spin</type>
    <object-name>prop_left</object-name>
    <property>velocities/true-airspeed-kt</property>
    <factor>6</factor>
    <center>
      <x-m> -4.6860 </x-m>
      <y-m> -3.7219 </y-m>
      <z-m> 2.4209 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation> 
  
  
